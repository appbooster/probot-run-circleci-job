{
  "name": "probot-run-circleci-job",
  "version": "1.0.0",
  "description": "A Probot app to run any defined job from circleci config for project",
  "author": "Alexandr Subbotin <kelionweb@gmail.com>",
  "repository": "https://github.com/appbooster/probot-run-circleci-job.git",
  "homepage": "https://github.com/appbooster/probot-run-circleci-job",
